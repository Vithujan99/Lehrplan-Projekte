		do Screen.drawRectangle(rectStart, 121, rectEnd, 132);
		return;
	}

	function void drawDitheredQuad(int x0, int m, int block, int space) {
		var int x, y, m, sf, x0, shifted;
		let shifted = 0;

		do Screen.setColor(false);
		while(y < 11) {
			let x = x0 + (shifted * (block + space - 1));
			while(x < m) {
				do Screen.drawLine(x, 121 + y, x + block - 1, 121 + y);
				let x = x + block + space;
			}
			let shifted = 1 - shifted;
			let y = y + 1;
		}

		return;
	}
}