	function void drawDogma() {
		var int wait, longWait;
		let wait = 30;
		let longWait = 2000;

		do Screen.setColor(false);
		do Screen.drawRectangle(0, 0, 511, 255);
		do MainMenu.fadeString("In the first age, in the first battle", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("When the shadows first lengthened", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("One stood", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("He chose the path of perpetual torment", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("In his ravenous hatred he found no peace", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("And with boiling blood he scoured the umbral plains", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("Seeking vengeance against the dark lords who had wronged him", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("And those that tasted the bite of his sword named him", wait, longWait);
		do Sys.wait(800);
		do MainMenu.fadeString("The Doom Slayer", wait, 8000);
		return;
	}

	function void fadeString(String s, int wait, int longWait) {
